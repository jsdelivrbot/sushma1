<html>
<style>
img { position: relative;
	  top: 1%;
	  left: 33%;
	  width: 350px;
	  height: 100px;
}
</style>
<head>
 <img src="/sibi.png">
</head>
<body style="background-color:Black">
 <div class="container">
   <form id="submitForm" action="/wc" method="get">   
   <fieldset>
   <font color="grey">
   <label for="name"><font color="grey">Name:</font></label>
   <input name="name" id="name" type="text" placeholder="Enter given name"> </input>
   <label for="surName"><font color="grey">SurName:</font></label>
   <input name="surName" id="surName" type="text" placeholder="Enter surname"> </input>
   <input type="submit" name="submit"/>
   </fieldset>
   </font>
   </form>
 </div>
 <div class="container" style="color:black">		  
  <font color="grey">
	<h2>List Of Records</h2>
	<ul>
		<% results.forEach(function(r) { %>
		<a href="/user/<%=r.number%>">
		<li><%= r.givenname %>  <%= r.surname %></li>
		</a>
		<% }); %>
	</ul>
  </font>
  </div>
  <div style="align:center;"> 
  <a href="/">Home</a>
  </div>
</body>
</html>